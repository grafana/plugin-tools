{
  "name": "{{ kebabCase pluginName }}",
  "version": "1.0.0",
  "scripts": {
    "build": "{{frontendBundler}} -c ./.config/{{frontendBundler}}/{{frontendBundler}}.config.ts --env production",
    "dev": "{{frontendBundler}} -w -c ./.config/{{frontendBundler}}/{{frontendBundler}}.config.ts --env development",
    "test": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache .",
    "lint:fix": "{{ packageManagerName }} run lint{{#if isNPM}} --{{/if}} --fix && prettier --write --list-different .",
    "e2e": "playwright test",
    "server": "docker compose up --build",
    "sign": "npx --yes @grafana/sign-plugin@latest"
  },
  "author": "{{ sentenceCase orgName }}",
  "license": "Apache-2.0",
  "devDependencies": {
    "@grafana/eslint-config": "^9.0.0",
    "@grafana/plugin-e2e": "^3.3.0",
    "@grafana/tsconfig": "^2.0.1",
    "@playwright/test": "^1.57.0",{{#if useExperimentalRspack}}
    "@rspack/core": "^1.6.0",
    "@rspack/cli": "^1.6.0",{{/if}}
    "@stylistic/eslint-plugin-ts": "^4.4.0",
    "@swc/core": "^1.15.0",
    "@swc/helpers": "^0.5.0",
    "@swc/jest": "^0.2.0",
    "@testing-library/jest-dom": "^6.6.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^24.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",{{#if useExperimentalRspack}}
    "@types/ws": "^8.18.1",{{/if}}
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",{{#unless useExperimentalRspack}}
    "copy-webpack-plugin": "^13.0.0",{{/unless}}
    "css-loader": "^7.1.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.0",
    "eslint-plugin-jsdoc": "^52.0.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-webpack-plugin": "^5.0.0",{{#unless useExperimentalRspack}}
    "fork-ts-checker-webpack-plugin": "^9.1.0",{{/unless}}
    "glob": "^11.1.0",
    "identity-obj-proxy": "^3.0.0",
    "imports-loader": "^5.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.6.0",
    "replace-in-file-webpack-plugin": "^1.0.0",{{#if useExperimentalRspack}}
    "rspack-plugin-virtual-module": "^1.0.0",{{/if}}
    "sass": "^1.89.0",
    "sass-loader": "^16.0.0",
    "semver": "^7.7.0",
    "style-loader": "^4.0.0",{{#unless useExperimentalRspack}}
    "swc-loader": "^0.2.0",{{/unless}}
    "terser-webpack-plugin": "^5.3.0",
    "ts-node": "^10.9.0",{{#if useExperimentalRspack}}
    "ts-checker-rspack-plugin": "^1.2.0",{{/if}}
    "typescript": "5.9.2",
    "webpack": "^5.101.0"{{#unless useExperimentalRspack}},
    "webpack-cli": "^6.0.0",
    "webpack-livereload-plugin": "^3.0.2",
    "webpack-subresource-integrity": "^5.1.0",
    "webpack-virtual-modules": "^0.6.2"{{/unless}}{{#if useExperimentalRspack}},
    "ws": "^8.13.0"{{/if}}
  },
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@emotion/css": "11.10.6",
    "@grafana/data": "^12.4.0",
    "@grafana/i18n": "^12.4.0",
    "@grafana/runtime": "^12.4.0",
    "@grafana/ui": "^12.4.0",
    "@grafana/schema": "^12.4.0",{{#if_eq pluginType "scenesapp" }}
    "@grafana/scenes": "{{ scenesVersion }}",{{/if_eq}}
    "react": "^18.3.0",
    "react-dom": "^18.3.0"{{#if isAppType}},
    "react-router-dom": "^6.22.0",
    "rxjs": "7.8.2"{{/if}}
  },
  "packageManager": "{{ packageManagerName }}@{{ packageManagerVersion }}"
}
