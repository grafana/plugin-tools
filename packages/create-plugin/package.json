{
  "name": "@grafana/create-plugin",
  "version": "5.22.2",
  "repository": {
    "directory": "packages/create-plugin",
    "url": "https://github.com/grafana/plugin-tools"
  },
  "author": "Grafana",
  "license": "Apache-2.0",
  "bin": "./dist/bin/run.js",
  "type": "module",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "clean": "rm -rf ./dist ./generated",
    "build": "rollup -c ../../rollup.config.ts --configPlugin esbuild",
    "dev": "npm run build -- -w",
    "lint": "eslint --cache ./src",
    "lint:fix": "npm run lint -- --fix",
    "lint:package": "publint",
    "test": "vitest --passWithNoTests",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "change-case": "^5.4.0",
    "debug": "^4.3.4",
    "enquirer": "^2.4.1",
    "find-up": "^7.0.0",
    "glob": "^11.0.0",
    "handlebars": "^4.7.8",
    "minimist": "^1.2.8",
    "semver": "^7.3.5",
    "title-case": "^4.3.0",
    "which": "^5.0.0",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@libs/output": "^1.0.2",
    "@libs/version": "^1.0.1",
    "@types/glob": "^8.1.0",
    "@types/minimist": "^1.2.5",
    "@types/semver": "^7.7.0",
    "@types/tmp": "^0.2.6",
    "@types/which": "^3.0.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "tmp": "^0.2.3"
  },
  "overrides": {
    "@types/marked-terminal": {
      "marked": "^15.0.0"
    }
  },
  "engines": {
    "node": ">=20"
  }
}
